<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> All users data </title>
    <style>
        table, th, td, tr {
            border: 1px solid black;
            border-collapse: collapse;
        }
        th, td {
            padding: 10px;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th> ID </th>
            <th> Username </th>
            <th> Email </th>
            <th> Password </th>
        </tr>
        <% for (let user of users) { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.username %></td>
                <td><%= user.email %></td>
                <td password="viewport"><%= user.password %></td>
                <td>
                 <form Method="GET" action="/users/<%= user.id %>/edit"> 
                    <button > Edit Username</button>
                 </form>
                </td>
            </tr>
        <% } %>
    </table>
</body>
</html>